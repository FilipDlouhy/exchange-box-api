      "root": "apps/item",
      "entryFile": "main",
      "sourceRoot": "apps/item/src",
      "compilerOptions": {
        "tsConfigPath": "apps/item/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
